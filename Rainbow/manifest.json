{
	"manifest_version": 2,
	"name": "FT prototype",
	"description": "New Tab Test",
	"version": "1.0",

	"chrome_url_overrides": {
		"newtab": "newpage.html"
	},

	"browser_action": {
		"default_icon": {
    		"16": "icons/star16.png",
            "24": "icons/star24.png",
            "32": "icons/star32.png"
    	}
	},

	"permissions": ["tabs"],

	"background": {
		"scripts": ["background.js"]
	}
}